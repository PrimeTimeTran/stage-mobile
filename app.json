{
  "expo": {
    "name": "Stage",
    "description": "Know those around you",
    "slug": "get-on-stage",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.getonstageapp.app"
    },
    "android": {
      "package": "com.getonstageapp.app"
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    }
  }
}
